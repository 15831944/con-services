<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <AssemblyName>VSS.Tile.Service.WebApi</AssemblyName>
    <OutputType>exe</OutputType>
    <PackageId>TileServiceWebApi</PackageId>
    <RootNamespace>VSS.Tile.Service.WebApi</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <DocumentationFile>bin\Debug\netcoreapp2.1\VSS.Tile.Service.WebApi.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.App" />
    <PackageReference Include="OpenTracing.Contrib.NetCore" Version="0.4.0" />
    <PackageReference Include="VSS.Log4NetExtensions.netcore" Version="2.3.3" />
    <PackageReference Include="VSS.WebApi.Common.netcore" Version="1.2.10" />
    <PackageReference Include="VSS.MasterData.Proxies.netcore" Version="2.48.1" />
    <PackageReference Include="VSS.MasterData.Models.netcore" Version="2.34.2" />
    <PackageReference Include="VSS.TCCFileAccess.netcore" Version="2.12.2" />
    <PackageReference Include="VSS.ConfigurationStore.netcore" Version="2.1.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\VSS.Tile.Service.Common\VSS.Tile.Service.Common.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="log4net.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
