{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [ "appsettings.json" ]
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        },
        "Microsoft.Extensions.DependencyInjection": "1.0.0",
        "KafkaConsumer": "*",
        "ConfigurationStore": "*",
        "log4net.netcore": "3.0.0",
        "log4netExtensions.netcore": "1.0.1"
      }
    }
  },
  "publishOptions": {
    "include": [
      "Dockerfile",
      "runme.sh",
      "wait-for-it.sh",
      "log4net.xml",
      "appsettings.json"
    ]
  },

  "dependencies": {
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "Repositories": "1.0.0-*"
  }
}
