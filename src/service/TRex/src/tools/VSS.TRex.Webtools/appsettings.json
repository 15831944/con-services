{
  "Serilog": {
    "LogLevel": {
      "Default": "Trace"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".\\logs\\Webtools.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": "1048576000"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
