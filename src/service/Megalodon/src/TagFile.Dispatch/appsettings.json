{
  "LogLevel": {
    "Default": "Debug",
    "Microsoft": "Warning",
    "OpenTracing.Contrib.NetCore.AspNetCore": "Warning",
    "System": "Warning"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:\\Trimble\\Logs\\TagFileService.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "TCIP": "127.0.0.1",
  "Port": "1500",
  "TagFileIntervalSecs": "60",
  "InstallFolder": "c:\\Trimble",
  "SendTagFilesDirect": true,
  "DebugTraceToLog": false,
  "TransmissionProtocolVersion": "1",
  "ApplicationVersion": "1.5",
  "production-host": "https://api.trimble.com",
  "production-base": "/t/trimble.com/ccss-tagfile-gateway/1.0"
}
