    <ProjectReference Include="..\WebApiCommon\VSS.Hydrology.WebApi.Common.csproj" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp2.1' ">
    <Compile Remove=".\Report\Controllers\ConfigController.cs" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net471' ">
    <Reference Include="System" />
