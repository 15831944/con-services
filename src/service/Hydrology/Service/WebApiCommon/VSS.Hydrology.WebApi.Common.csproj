<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net471;netcoreapp2.1</TargetFrameworks>
    <AssemblyName>VSS.Hydrology.WebApi.Common</AssemblyName>
    <PackageId>WebApiCommon</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>VSS.Hydrology.WebApi.Common</RootNamespace>
    <Platforms>x64</Platforms>
    <PackageTags>vss-hydrology-service</PackageTags>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Coord\ResultHandling\**" />
    <Compile Remove="DXF\**" />
    <EmbeddedResource Remove="Coord\ResultHandling\**" />
    <EmbeddedResource Remove="DXF\**" />
    <None Remove="Coord\ResultHandling\**" />
    <None Remove="DXF\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Polly" Version="6.0.1" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Hosting.WindowsServices" Version="2.2.0" />
    <PackageReference Include="DotNetCore.CAP" Version="2.3.0" />
    <PackageReference Include="DotNetCore.CAP.Kafka" Version="2.3.0" />
    <PackageReference Include="DotNetCore.CAP.MySql" Version="2.3.0" />
    <PackageReference Include="System.Net.Security" Version="4.3.2" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
    <PackageReference Include="AutoMapper" Version="8.0.0" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Common\MasterDataModels\src\VSS.MasterData.Models.csproj" />
    <ProjectReference Include="..\..\..\..\Common\MasterDataProxies\src\VSS.MasterData.Proxies.csproj" />
    <ProjectReference Include="..\..\Clients\Abstractions\VSS.Hydrology.WebApi.Abstractions.csproj" />
    <ProjectReference Include="..\TTM\VSS.Hydrology.WebApi.TTM.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="log4net">
      <HintPath>..\..\..\..\Common\log4netExtensions\src\dll\log4net.dll</HintPath>
    </Reference>
    <Reference Include="Morph.Contracts">
      <HintPath>..\HydroLibraries\Morph.Contracts.dll</HintPath>
    </Reference>
    <Reference Include="Morph.Core">
      <HintPath>..\HydroLibraries\Morph.Core.dll</HintPath>
    </Reference>
    <Reference Include="Morph.Module.Services">
      <HintPath>..\HydroLibraries\Morph.Module.Services.dll</HintPath>
    </Reference>
    <Reference Include="Morph.Service.Core">
      <HintPath>..\HydroLibraries\Morph.Service.Core.dll</HintPath>
    </Reference>
    <Reference Include="Morph.Service.Engine">
      <HintPath>..\HydroLibraries\Morph.Service.Engine.dll</HintPath>
    </Reference>

    <!-- only needed if using sketup for pondmap generation 
    <Reference Include="Trimble.Vce.Data.Skp">
      <HintPath>..\HydroLibraries\Trimble.Vce.Data.Skp.dll</HintPath>
    </Reference>
    -->

    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="RecomputeDimBlock_4.00_11.tx">
      <HintPath>..\HydroLibraries\RecomputeDimBlock_4.00_11.tx</HintPath>
    </Reference>
    <Reference Include="WindowsBase" />

  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <WarningsAsErrors />
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <WarningsAsErrors />
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <WarningsAsErrors />
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(TargetFramework)' == 'net471' ">
    <DefineConstants>$(DefineConstants);STRONG;NET;NET_2_0;NET_4_0;NET_4_5;NET_4_7</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

</Project>
