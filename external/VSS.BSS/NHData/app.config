<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="VSS.Nighthawk.NHDataSvc.NHDataSettings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <section name="DynamicProxy" type="VSS.Nighthawk.Instrumentation.Config.DynamicProxyConfigSection, VSS.Nighthawk.Instrumentation" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <log4net>
    <appender name="MainAppender" type="VSS.Hosted.VLCommon.LogAppender, VSS.Hosted.VLCommon">
      <file value="NHDataSvc/NHDataSvc.log" />
      <threshold value="INFO" />
    </appender>
    <appender name="ErrorAppender" type="VSS.Hosted.VLCommon.LogAppender, VSS.Hosted.VLCommon">
      <file value="Errors/Error.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <threshold value="ERROR" />
    </appender>
    <appender name="AlarmAppender" type="VSS.Hosted.VLCommon.AppAlarmAppender, VSS.Hosted.VLCommon" />
    <appender name="SQLTraceAppender" type="VSS.Hosted.VLCommon.LogAppender, VSS.Hosted.VLCommon">
      <file value="SQLTrace/SQLTrace.xml" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline" />
      </layout>
    </appender>
    <appender name="MTAppender" type="VSS.Hosted.VLCommon.LogAppender, VSS.Hosted.VLCommon">
      <file value="NHDataSvc/MassTransit.log" />
      <threshold value="INFO" />
    </appender>
    <renderer renderingClass="VSS.Hosted.VLCommon.ExceptionRenderer, VSS.Hosted.VLCommon" renderedClass="System.Exception" />
    <logger name="VSS">
      <appender-ref ref="MainAppender" />
      <appender-ref ref="ErrorAppender" />
      <appender-ref ref="AlarmAppender" />
    </logger>
    <logger name="SQLTraceLogger">
      <appender-ref ref="SQLTraceAppender" />
    </logger>
    <logger name="MassTransit">
      <appender-ref ref="MTAppender" />
      <appender-ref ref="ErrorAppender" />
    </logger>
  </log4net>
  <appSettings>
    <add key="DatabaseUser" value="_NHDataSvc" />
    <add key="DatabasePassword" value="!uM7bra_" />
    <add key="Service.RecoveryActions" value="restart/60000/restart/60000/restart/60000" />
    <add key="MetricsInstrumentation" value="0" />
    <add key="RabbitMqUser" value="_NHDataSvc" />
    <add key="RabbitMqPassword" value="d67K$Q!zQUGjQM" />
    <add key="RabbitMqVirtualHost" value="VisionLink" />
    <add key="RabbitMqName" value="HA.NHDataSvc" />
    <add key="RabbitMqHeartbeatSeconds" value="60" />
    <!--Oculus/TCA: Vehicle Mapping cache refresh interval period-->
    <add key="VehicleMappingCacheRefreshInterval" value="00:03:00" />
    <!-- Uncomment below to enable tracing of all LINQ-to-entities generated SQL to SqlTrace.xml log-->
    <!--<remove key="EnableQueryTracing"/><add key="EnableQueryTracing" value="true"/>-->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="CatMakeCodes" value="CAT,F80,O80" />
    <add key="CacheUpdateThreshold" value="00:03:00" />
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="VSS.Nighthawk.NHDataSvc.NHDataProcessor" behaviorConfiguration="NHDataSvcBehavior">
        <endpoint address="net.msmq://./private/NHData" binding="netMsmqBinding" bindingConfiguration="mq-no-security" name="NHData" contract="VSS.Hosted.VLCommon.INHDataProcessor" />
        <endpoint address="net.pipe://localhost/NHDataPipe" binding="netNamedPipeBinding" contract="VSS.Hosted.VLCommon.INHDataProcessor" bindingConfiguration="np-no-security" />
        <endpoint address="net.tcp://localhost:8000/RemoteNhData" binding="netTcpBinding" bindingConfiguration="tcpBinding" name="RemoteNhData" contract="VSS.Hosted.VLCommon.INHDataProcessor" />
      </service>
    </services>
    <client>
      <endpoint address="net.msmq://./private/NHAddressData" binding="netMsmqBinding" behaviorConfiguration="EndPointServiceViewEventBehavior" bindingConfiguration="mq-no-security" contract="VSS.Hosted.VLCommon.IAddress" name="NHAddress" />
      <endpoint address="net.msmq://./private/NHSiteData" binding="netMsmqBinding" behaviorConfiguration="EndPointServiceViewEventBehavior" bindingConfiguration="mq-no-security" contract="VSS.Hosted.VLCommon.ISiteDispatch" name="NHSiteDispatch" />
      <endpoint address="net.msmq://./private/NHAlertData" binding="netMsmqBinding" behaviorConfiguration="EndPointServiceViewEventBehavior" bindingConfiguration="mq-no-security" contract="VSS.Hosted.VLCommon.IAlertTrigger" name="NHAlertData" />
      <endpoint address="net.msmq://./private/NHConfigStatus" behaviorConfiguration="EndPointServiceViewEventBehavior" binding="netMsmqBinding" bindingConfiguration="mq-no-security" contract="VSS.Hosted.VLCommon.IConfigStatus" name="NHConfigStatusSvc" />
    </client>
    <bindings>
      <netMsmqBinding>
        <binding name="mq-no-security" receiveErrorHandling="Fault" maxReceivedMessageSize="4194304">
          <security>
            <transport msmqAuthenticationMode="None" msmqProtectionLevel="None" />
          </security>
        </binding>
      </netMsmqBinding>
      <netNamedPipeBinding>
        <binding name="np-no-security" maxReceivedMessageSize="4194304" closeTimeout="1" maxConnections="750" maxBufferPoolSize="524288" maxBufferSize="4194304" transferMode="Buffered" sendTimeout="5" receiveTimeout="10" openTimeout="1" transactionFlow="false" transactionProtocol="OleTransactions">
          <security mode="None">
            <transport protectionLevel="None" />
          </security>
        </binding>
      </netNamedPipeBinding>
      <netTcpBinding>
        <binding name="tcpBinding" portSharingEnabled="true">
          <security mode="None" />
        </binding>
        <!--
                 closeTimeout="00:01:00" 
                 openTimeout="00:01:00" 
                 receiveTimeout="00:10:00" 
                 sendTimeout="00:01:00" 
        -->
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="NHDataSvcBehavior">
          <serviceDebug includeExceptionDetailInFaults="false" />
          <dataContractSerializer maxItemsInObjectGraph="4194304" />
          <serviceThrottling maxConcurrentCalls="100" maxConcurrentSessions="100" maxConcurrentInstances="100" />
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="EndPointServiceViewEventBehavior">
          <dataContractSerializer maxItemsInObjectGraph="4194304" />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <spring>
    <context>
      <resource uri="config://spring/objects" />
    </context>
    <objects xmlns="http://www.springframework.net" xmlns:aop="http://www.springframework.net/aop">
      <object id="AppMetricsRecorderAdvisor" type="Spring.Aop.Support.NameMatchMethodPointcutAdvisor, Spring.Aop">
        <property name="Advice">
          <object type="VSS.Nighthawk.Instrumentation.AppMetricsRecorderAdvisor, VSS.Nighthawk.Instrumentation" />
        </property>
        <property name="MappedNames">
          <list>
            <!-- NHDataProcessor -->
            <value>Process</value>
            <!--<value>ForwardSiteDispatchData</value>
            <value>ForwardPLLocationData</value>
            <value>ForwardAlertableItems</value>-->
            <!-- NHDataSaver -->
            <!--<value>SaveViaInstance</value>-->
          </list>
        </property>
      </object>
      <object id="OperationLatencyMetricAdvisor" type="Spring.Aop.Support.NameMatchMethodPointcutAdvisor, Spring.Aop">
        <property name="Advice">
          <object type="VSS.Nighthawk.Instrumentation.OperationLatencyMetricAdvisor, VSS.Nighthawk.Instrumentation" />
        </property>
        <property name="MappedNames">
          <list>
            <!-- NHDataProcessor -->
            <value>Process</value>
            <value>ForwardSiteDispatchData</value>
            <value>ForwardPLLocationData</value>
            <value>ForwardAlertableItems</value>
            <!-- NHDataSaver -->
            <value>SaveViaInstance</value>
          </list>
        </property>
      </object>
      <object id="OperationRateMetricAdvisor" type="Spring.Aop.Support.NameMatchMethodPointcutAdvisor, Spring.Aop">
        <property name="Advice">
          <object type="VSS.Nighthawk.Instrumentation.OperationRateMetricAdvisor, VSS.Nighthawk.Instrumentation" />
        </property>
        <property name="MappedNames">
          <list>
            <!-- NHDataProcessor -->
            <value>Process</value>
            <value>ForwardSiteDispatchData</value>
            <value>ForwardPLLocationData</value>
            <value>ForwardAlertableItems</value>
            <!-- NHDataSaver -->
            <value>SaveViaInstance</value>
          </list>
        </property>
      </object>
      <object id="VSS.Nighthawk.NHDataSvc.NHDataProcessor" type="Spring.Aop.Framework.ProxyFactoryObject, Spring.Aop">
        <property name="ExposeProxy" value="true" />
        <property name="ProxyTargetType" value="true" />
        <property name="Target">
          <object type="VSS.Nighthawk.NHDataSvc.NHDataProcessor, VSS.Nighthawk.NHDataSvc" />
        </property>
        <property name="InterceptorNames">
          <list>
            <value>AppMetricsRecorderAdvisor</value>
            <value>OperationLatencyMetricAdvisor</value>
            <value>OperationRateMetricAdvisor</value>
          </list>
        </property>
      </object>
      <object id="VSS.Hosted.VLCommon.NHDataSaver" type="Spring.Aop.Framework.ProxyFactoryObject, Spring.Aop">
        <property name="ExposeProxy" value="true" />
        <property name="Target">
          <object type="VSS.Hosted.VLCommon.NHDataSaver, VSS.Hosted.VLCommon" />
        </property>
        <property name="InterceptorNames">
          <list>
            <!--<value>AppMetricsRecorderAdvisor</value>-->
            <value>OperationLatencyMetricAdvisor</value>
            <value>OperationRateMetricAdvisor</value>
          </list>
        </property>
      </object>
    </objects>
  </spring>
  <applicationSettings>
    <VSS.Nighthawk.NHDataSvc.NHDataSettings>
      <setting name="SuspensionInterval" serializeAs="String">
        <value>00:05:00</value>
      </setting>
      <setting name="MaxRetries" serializeAs="String">
