      </setting>
      <setting name="ProcessStuckInterval" serializeAs="String">
        <value>00:05:00</value>
      </setting>
      <setting name="ConsumerLimit" serializeAs="String">
        <value>50</value>
      </setting>
    </VSS.Nighthawk.NHDataSvc.NHDataSettings>
  </applicationSettings>
