    <sql>DECLARE &#x40;InsertUTC &#x5b;datetime&#x5d; &#x3d; &#x3f;, &#x40;SentUTC &#x5b;datetime&#x5d; &#x3d; &#x3f;, &#x40;Status &#x5b;int&#x5d; &#x3d; &#x3f;, &#x40;Payload &#x5b;varbinary&#x5d;&#x28;4000&#x29; &#x3d; &#x3f;, &#x40;SequenceID &#x5b;bigint&#x5d; &#x3d; &#x3f;, &#x40;IsAck &#x5b;bit&#x5d; &#x3d; &#x3f;, &#x40;SentCount &#x5b;tinyint&#x5d; &#x3d; &#x3f;, &#x40;PacketID &#x5b;int&#x5d; &#x3d; &#x3f;, &#xd;&#xa;&#x40;TypeID &#x5b;int&#x5d; &#x3d; &#x3f;, &#x40;SerialNumber &#x5b;varchar&#x5d;&#x28;128&#x29; &#x3d; &#x3f;, &#x40;DueUTC &#x5b;datetime2&#x5d;&#x28;7&#x29; &#x3d; &#x3f;, &#x40;SubTypeID &#x5b;int&#x5d; &#x3d; &#x3f;, &#x40;DeviceType &#x5b;int&#x5d; &#x3d; &#x3f;,&#x40;NewSequenceID bigint,&#x40;NewPayload &#x5b;varbinary&#x5d;&#x28;4000&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;IF &#x28;&#x40;DeviceType &#x3d; 5&#x29;  &#xd;&#xa;	BEGIN&#xd;&#xa;		SELECT &#x40;NewSequenceID &#x3d; CCOutSequenceID &#x2b; 1 FROM &#x5b;Sequence&#x5d;&#x3b;&#xd;&#xa;		UPDATE &#x5b;Sequence&#x5d; set CCOutSequenceID &#x3d; &#x40;NewSequenceID&#x3b;&#xd;&#xa;	END&#xd;&#xa;ELSE&#xd;&#xa;	BEGIN&#xd;&#xa;		SELECT &#x40;NewSequenceID &#x3d; PR3OutSequenceID &#x2b; 1 FROM &#x5b;Sequence&#x5d;&#x3b;&#xd;&#xa;		UPDATE &#x5b;Sequence&#x5d; set PR3OutSequenceID &#x3d; &#x40;NewSequenceID&#x3b;&#xd;&#xa;	END&#xd;&#xa;&#xd;&#xa;IF &#x28;len&#x28;&#x40;Payload&#x29; &#x3e; 7 and &#x40;SequenceID is not null&#x29;&#xd;&#xa;	BEGIN&#xd;&#xa;		SELECT &#x40;NewPayload &#x3d;  cast&#x28;stuff&#x28;&#x40;Payload,5,4,cast&#x28;Reverse&#x28;CONVERT&#x28;VARBINARY&#x28;4&#x29;, &#x40;NewSequenceID&#x29;&#x29; as varbinary&#x28;4&#x29;&#x29;&#x29; as varbinary&#x28;4000&#x29;&#x29;&#x3b;&#xd;&#xa;		INSERT INTO MTSOUT&#x28;InsertUTC,SentUTC,Status,Payload,SequenceID,IsAck,SentCount,PacketID,TypeID,SerialNumber,DueUTC,SubTypeID,DeviceType&#x29; &#xd;&#xa;					VALUES &#x28;&#x40;InsertUTC,&#x40;SentUTC,&#x40;Status,&#x40;NewPayload,&#x40;NewSequenceID,&#x40;IsAck,&#x40;SentCount,&#x40;PacketID,&#x40;TypeID,&#x40;SerialNumber,&#x40;DueUTC,&#x40;SubTypeID,&#x40;DeviceType&#x29;&#xd;&#xa;	END&#xd;&#xa;ELSE&#xd;&#xa;	BEGIN&#xd;&#xa;		INSERT INTO MTSOUT&#x28;InsertUTC,SentUTC,Status,Payload,SequenceID,IsAck,SentCount,PacketID,TypeID,SerialNumber,DueUTC,SubTypeID,DeviceType&#x29; &#xd;&#xa;					VALUES &#x28;&#x40;InsertUTC,&#x40;SentUTC,&#x40;Status,&#x40;Payload,&#x40;SequenceID,&#x40;IsAck,&#x40;SentCount,&#x40;PacketID,&#x40;TypeID,&#x40;SerialNumber,&#x40;DueUTC,&#x40;SubTypeID,&#x40;DeviceType&#x29;&#xd;&#xa;	END&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#x40;NewSequenceID ,&#x40;NewPayload,&#x40;Payload&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
